pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compile in progress'
                

                publishHTML([allowMissing: false, 
                             alwaysLinkToLastBuild: false, 
                             keepAll: false, 
                             reportDir: '',  
                             reportFiles: 'hello.html',  
                             reportName: 'HTML_Report'])  
            }
        }
    }
}
